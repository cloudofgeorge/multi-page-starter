{
  "name": "Multi page starter",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "start": "webpack serve -c ./webpack/webpack.dev.js",
    "build": "webpack -c ./webpack/webpack.prod.js",
    "clean": "npm run clean:build & npm run clean:cache",
    "clean:cache": "rimraf ./webpack/cache",
    "clean:build": "rimraf dist",
    "lint": "npm run lint:js && npm run prettier:check && npm run lint:styles",
    "lint:js": "eslint 'src/*/**/*.{js,ts,json}'",
    "lint:styles": "stylelint 'src/**/*.{css,scss}'",
    "lint:styles:fix": "npm run lint:styles -- --fix",
    "prettier:check": "prettier --config ./.prettierrc --list-different \"src/**/*.{js,ts,json,css,scss}\"",
    "prettier:fix": "prettier --config ./.prettierrc --write \"src/**/*.{js,ts,json,css,scss}\"",
    "fix:js": "npm run lint:js -- --fix",
    "fix:all": "npm run fix:js && npm run prettier:fix",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch --pretty",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u",
    "dev:analyze": "npm run dev -- --env.addons=bundleanalyzer",
    "analyze": "webpack --analyze -c webpack/scripts/analyze.js",
    "http-server": "http-server -c-1 dist",
    "postinstall": "simple-git-hooks",
    "cricical-css:generate": "node ./webpack/scripts/critical-css.js"
  },
  "simple-git-hooks": {
    "pre-commit": "npx pretty-quick --staged && git add .",
    "pre-push": "npx lint-staged"
  },
  "lint-staged": {
    "{src}/**/*.{js,ts}": [
      "npm run lint:js"
    ],
    "src/**/*.{css,scss}": [
      "npm run lint:styles"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudofgeorge/Multi page starter.git"
  },
  "author": "cloudofgeorge",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cloudofgeorge/Multi page starter/issues"
  },
  "homepage": "https://github.com/cloudofgeorge/Multi page starter#readme",
  "dependencies": {
    "handlebars": "^4.7.7",
    "nanoid": "^3.1.23"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/runtime": "^7.17.8",
    "@types/jest": "^27.4.1",
    "@types/nanoid": "^3.0.0",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "autoprefixer": "^10.3.6",
    "babel-loader": "^8.2.4",
    "circular-dependency-plugin": "^5.2.2",
    "critical": "^4.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "dotenv-webpack": "^7.1.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-typescript": "^16.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.6",
    "handlebars-loader": "^1.7.3",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "image-minimizer-webpack-plugin": "^3.0.1",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "jest": "^27.5.1",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "postcss-custom-media": "^8.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.4.3",
    "postcss-scss": "^4.0.3",
    "preload-webpack-plugin": "^2.3.0",
    "prettier": "^2.6.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "sass": "^1.49.10",
    "sass-loader": "^12.6.0",
    "simple-git-hooks": "^2.7.0",
    "style-loader": "^3.3.0",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.2.0",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3",
    "webpack": "^5.70.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "5.8.0",
    "webpack-pwa-manifest": "^4.3.0",
    "workbox-webpack-plugin": "6.4.2"
  }
}
